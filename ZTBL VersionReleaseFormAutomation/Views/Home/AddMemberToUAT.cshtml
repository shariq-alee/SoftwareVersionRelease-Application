        <form method="post" action="AddProject" style="padding:30px">
            <b> @Html.LabelFor(model => model.PPNo)</b> @Html.TextBoxFor(model => model.PPNo, new { @class = "form-control", style = "width:300px", placeholder = "Enter PP Number", @required = "required" })<br />
            <b> @Html.LabelFor(model => model.Name)</b> @Html.TextBoxFor(model => model.Name, new { @class = "form-control", style = "width:300px", placeholder = "Enter Name", @required = "required" })<br />
            <b>@Html.LabelFor(model => model.Designation)</b> @Html.TextBoxFor(model => model.Designation, new { @class = "form-control", @required = "required", placeholder = "Designation", id = "filesize", style = "width:100px" })
            <b> @Html.Label("UAT Form Id")</b> @Html.TextBoxFor(model => model.UATFormId, new { @class = "form-control", @required = "required", placeholder = "Enter File type (e.g: .exe)", style = "width:200px" })<br />

            <input type="submit" value="Add to list" onclick="enableTB()" class="btn-success" />
        </form>

